
@{
    ViewBag.Title = "Tokens";
}

<dl>
    @foreach (var claim in System.Security.Claims.ClaimsPrincipal.Current.Claims)
    {
        <dt>
            @claim.Type
        </dt>

        <dd>
            @claim.Value
        </dd>
    }
</dl>

<p>
    @Html.ActionLink("API Call service", "ApiCallService", "App")
</p>

